<?xml version="1.0" encoding="utf-8"?>
<androidx.swiperefreshlayout.widget.SwipeRefreshLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipe_refresh_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/mainContaner"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#f1f1f1">
        <!--    android:fitsSystemWindows="true"-->


        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/app_bar_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/transparent"
            android:fitsSystemWindows="true"
            android:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

            <com.google.android.material.appbar.CollapsingToolbarLayout
                android:id="@+id/collapsing_toolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fitsSystemWindows="true"
                app:layout_scrollFlags="scroll|exitUntilCollapsed">

                <ImageView

                    android:id="@+id/image"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:fitsSystemWindows="true"
                    android:scaleType="centerCrop"
                    android:src="@drawable/download_new"
                    android:visibility="gone"
                    app:layout_collapseMode="parallax" />

                <RelativeLayout
                    android:id="@+id/backCardArea"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_150sdp"
                    android:background="@color/appThemeColor_1" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:id="@+id/bannerArea"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_165sdp"
                        android:layout_marginTop="@dimen/_20sdp"
                        android:background="#EEEEEE"
                        android:visibility="gone"
                        app:layout_collapseMode="parallax">

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_90sdp"
                            android:background="@color/appThemeColor_1" />


                        <androidx.viewpager.widget.ViewPager
                            android:id="@+id/bannerViewPager"
                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_160sdp"
                            android:layout_alignParentBottom="true"
                            android:clipToPadding="false"
                            android:overScrollMode="never"
                            android:paddingStart="@dimen/card_padding"
                            android:paddingEnd="@dimen/card_padding" />

                        <com.view.LoopingCirclePageIndicator
                            android:id="@+id/bannerCirclePageIndicator"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_alignParentBottom="true"
                            android:layout_marginBottom="15dp"
                            android:padding="10dp"
                            android:visibility="gone" />


                        <LinearLayout
                            android:id="@+id/locationPopup"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:padding="10dp"
                            android:visibility="gone">

                            <ImageView
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:layout_marginStart="40dp"
                                app:srcCompat="@drawable/triangle">

                            </ImageView>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/rectangle"
                                android:orientation="vertical"
                                android:padding="15dp">


                                <com.view.MTextView
                                    android:id="@+id/noLocTitleTxt"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true"
                                    android:gravity="start"
                                    android:text="Have you selected the right location ?"
                                    android:textColor="@color/white"
                                    android:textSize="@dimen/txt_size_16"
                                    android:textStyle="bold" />

                                <com.view.MTextView
                                    android:id="@+id/noLocMsgTxt"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="false"
                                    android:layout_marginTop="7dp"
                                    android:gravity="start|center"
                                    android:paddingBottom="5dp"
                                    android:text="Your selected location seems to be little far off from your device location."
                                    android:textColor="#a8a8a8"
                                    android:textSize="@dimen/txt_size_16" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal">

                                    <com.view.MButton
                                        android:id="@+id/noThanksBtn"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginEnd="6dp"
                                        android:layout_weight="1"
                                        android:background="#6B6B6B"
                                        android:minHeight="45dp"
                                        android:text="No, Thanks"
                                        android:textColor="@color/white"
                                        android:textSize="@dimen/txt_size_16" />

                                    <com.view.MButton
                                        android:id="@+id/changeLocBtn"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="6dp"
                                        android:layout_weight="1"
                                        android:background="#6B6B6B"
                                        android:minHeight="45dp"
                                        android:text="Change Location"
                                        android:textColor="@color/white" />

                                </LinearLayout>

                            </LinearLayout>
                        </LinearLayout>
                    </RelativeLayout>


                    <LinearLayout
                        android:id="@+id/main_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15sdp"
                        android:layout_marginTop="@dimen/_55sdp"
                        android:layout_marginEnd="@dimen/_15sdp"
                        android:minHeight="@dimen/_120sdp"
                        android:visibility="visible">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:elevation="@dimen/_2sdp"
                            android:orientation="vertical"

                            android:visibility="visible"
                            app:layout_collapseMode="pin">

                            <LinearLayout
                                android:id="@+id/resNameView"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="8dp"
                                android:orientation="vertical">

                                <com.view.MTextView
                                    android:id="@+id/restNametxt"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_marginStart="@dimen/_10sdp"
                                    android:layout_marginTop="@dimen/_5sdp"
                                    android:layout_marginEnd="@dimen/_10sdp"
                                    android:ellipsize="marquee"
                                    android:gravity="center"
                                    android:includeFontPadding="false"
                                    android:marqueeRepeatLimit="marquee_forever"
                                    android:maxLines="2"
                                    android:textAlignment="center"
                                    android:textColor="@color/black"
                                    android:textSize="@dimen/_21ssp"
                                    app:customTypeFace="@string/robotobold"
                                    tools:text="Food On Way" />

                                <com.view.MTextView
                                    android:id="@+id/restcusineNameTxt"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_10sdp"
                                    android:layout_marginEnd="@dimen/_10sdp"
                                    android:gravity="center"
                                    android:includeFontPadding="false"
                                    android:maxLines="2"
                                    android:textColor="#646464"
                                    android:textSize="@dimen/_10ssp"
                                    app:customTypeFace="@string/systemRegular"
                                    tools:text="french" />

                            </LinearLayout>


                            <LinearLayout
                                android:id="@+id/resDetails"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:visibility="visible">


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:paddingTop="5dp"
                                    android:paddingBottom="5dp">


                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">

                                            <LinearLayout
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:gravity="center"
                                                android:orientation="horizontal">

                                                <ImageView
                                                    android:layout_width="@dimen/_20sdp"
                                                    android:layout_height="@dimen/_20sdp"
                                                    android:tint="@color/appThemeColor_1"
                                                    app:srcCompat="@drawable/ic_star_black_24dp" />

                                                <com.view.MTextView
                                                    android:id="@+id/ratingCntTxt"
                                                    android:layout_width="wrap_content"
                                                    android:layout_height="wrap_content"
                                                    android:gravity="center"
                                                    android:text=""
                                                    android:textColor="#141414"
                                                    android:textSize="@dimen/_15ssp"
                                                    app:customTypeFace="@string/systemMediumFont" />
                                            </LinearLayout>

                                            <com.view.MTextView
                                                android:id="@+id/ratingdecTxt"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:gravity="center"
                                                android:text=""
                                                android:textColor="#a0a0a0"
                                                android:textSize="@dimen/_10ssp"
                                                app:customTypeFace="@string/systemLightFont" />

                                        </LinearLayout>

                                    </LinearLayout>


                                    <View
                                        android:layout_width="@dimen/_1sdp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray"

                                        />

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">

                                            <com.view.MTextView
                                                android:id="@+id/deliveryValTxt"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:gravity="center"
                                                android:text=""
                                                android:textColor="#141414"
                                                android:textSize="@dimen/_15ssp"
                                                app:customTypeFace="@string/systemMediumFont" />

                                            <com.view.MTextView
                                                android:id="@+id/deliveryLBLTxt"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:gravity="center"
                                                android:text=""
                                                android:textColor="#a0a0a0"
                                                android:textSize="@dimen/_10ssp"
                                                app:customTypeFace="@string/systemLightFont" />

                                        </LinearLayout>

                                    </LinearLayout>

                                    <View
                                        android:id="@+id/minView"
                                        android:layout_width="@dimen/_1sdp"
                                        android:layout_height="match_parent"
                                        android:background="@color/gray"

                                        />

                                    <LinearLayout
                                        android:id="@+id/minOrderArea"
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1">

                                        <LinearLayout
                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:orientation="vertical">

                                            <com.view.MTextView
                                                android:id="@+id/minOrderValTxt"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:gravity="center"
                                                android:text=""
                                                android:textColor="#141414"
                                                android:textSize="@dimen/_15ssp"
                                                app:customTypeFace="@string/systemMediumFont" />

                                            <com.view.MTextView
                                                android:id="@+id/minOrderLBLTxt"
                                                android:layout_width="match_parent"
                                                android:layout_height="wrap_content"
                                                android:gravity="center"
                                                android:text=""
                                                android:textColor="#a0a0a0"
                                                android:textSize="@dimen/_10ssp"
                                                app:customTypeFace="@string/systemLightFont" />
                                        </LinearLayout>

                                    </LinearLayout>

                                </LinearLayout>


                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/offerArea"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:visibility="gone">

                                <com.view.MTextView
                                    android:id="@+id/offerMsgTxt"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center_horizontal"
                                    android:gravity="center"
                                    android:maxLines="2"
                                    android:paddingStart="10dp"
                                    android:paddingTop="5dp"
                                    android:paddingBottom="5dp"
                                    android:textAlignment="center"
                                    android:textColor="#8a584b"
                                    android:textSize="@dimen/txt_size_12"
                                    app:customTypeFace="@string/robotomediumFont" />

                            </LinearLayout>
                            <LinearLayout
                                android:padding="@dimen/_3sdp"
                                android:background="@drawable/safety_border"
                                android:layout_marginBottom="@dimen/_8sdp"
                                android:gravity="center_horizontal"
                                android:layout_gravity="center_horizontal"
                                android:visibility="gone"
                                android:id="@+id/safetylayout"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginEnd="@dimen/_10sdp"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <LinearLayout
                                    android:layout_marginStart="@dimen/_8sdp"
                                    android:gravity="center|center_horizontal"
                                    android:layout_weight="1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent">
                                <ImageView
                                    android:id="@+id/safetyImage"
                                    android:layout_gravity="center_vertical"
                                    android:src="@drawable/ic_safety"
                                    android:layout_width="@dimen/_12sdp"
                                    android:layout_height="@dimen/_12sdp"/>
                                <LinearLayout

                                    android:layout_marginStart="@dimen/_4sdp"
                                    android:layout_gravity="center_vertical|center_horizontal"
                                    android:orientation="vertical"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content">
                                    <com.view.MTextView
                                        android:layout_gravity="center_vertical"
                                        android:id="@+id/resSsafetydetailTxt"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_4sdp"
                                        android:layout_marginEnd="@dimen/_10sdp"
                                        android:gravity="start|center"
                                        android:includeFontPadding="false"
                                        android:maxLines="2"
                                        android:minLines="2"
                                        android:singleLine="true"
                                        android:text="Follows WHO safety practices"
                                        android:textAlignment="viewStart"
                                        android:textAllCaps="false"
                                        android:textColor="#426980"
                                        android:textSize="@dimen/txt_size_10"
                                        android:visibility="visible"
                                        app:customTypeFace="@string/robotomediumFont"/>

                                    <com.view.MTextView
                                        android:layout_gravity="center_vertical"
                                        android:id="@+id/resSsafetymeasureTxt"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/_4sdp"
                                        android:layout_marginEnd="@dimen/_10sdp"
                                        android:gravity="start|center"
                                        android:includeFontPadding="false"
                                        android:maxLines="1"
                                        android:singleLine="true"
                                        android:text="Follows WHO safety practices"
                                        android:textAlignment="viewStart"
                                        android:textAllCaps="false"
                                        android:textColor="#999A9A"
                                        android:textSize="@dimen/txt_size_10"
                                        android:visibility="gone"
                                        app:customTypeFace="@string/robotomediumFont"/>

                                </LinearLayout>
                                </LinearLayout>

                                <ImageView
                                    android:layout_gravity="end"
                                    app:srcCompat="@drawable/ic_arrow_right"
                                    android:id="@+id/arrowImage"
                                    android:layout_width="@dimen/_20sdp"
                                    android:layout_height="@dimen/_20sdp"/>


                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"

                        android:layout_marginTop="@dimen/_10sdp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_15sdp"
                            android:orientation="horizontal">

                            <com.view.MTextView
                                android:includeFontPadding="false"
                                android:id="@+id/reCommTxt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="start"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="@color/black"
                                android:textSize="@dimen/_16ssp"
                                android:visibility="gone"
                                app:customTypeFace="@string/systemSemiBold" />

                            <LinearLayout

                                android:id="@+id/VegNovegFilterArea"

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"

                                android:gravity="end"
                                android:orientation="horizontal"

                                android:visibility="gone">

                                <com.view.MTextView
                                    android:id="@+id/foodTypetxt"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_gravity="center_horizontal"

                                    android:gravity="start|center"
                                    android:paddingStart="10dp"
                                    android:text="Veg Only"
                                    android:textAlignment="viewStart"
                                    android:textAllCaps="true"
                                    android:textColor="#404040"
                                    android:textSize="@dimen/_11ssp"
                                    app:customTypeFace="@string/systemRegular" />

                                <com.kyleduo.switchbutton.SwitchButton
                                    android:id="@+id/vegNonVegSwitch"
                                    style="@style/SwitchButtonStyle"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    app:kswBackColor="@android:color/white"
                                    app:kswThumbColor="@android:color/holo_red_dark" />

                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_5sdp"
                            android:layout_marginBottom="@dimen/_5sdp"
                            android:orientation="horizontal">

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/resRecomMenuRecyclerview"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:clipToPadding="false"
                                android:orientation="horizontal"
                                android:paddingStart="@dimen/_20sdp"
                                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbar"
                    android:layout_width="match_parent"
                    android:layout_height="?attr/actionBarSize"
                    android:background="@color/appThemeColor_1"
                    android:contentInsetStart="0dp"
                    android:contentInsetEnd="0dp"
                    android:contentInsetRight="0dp"
                    app:contentInsetEnd="0dp"
                    app:contentInsetLeft="0dp"
                    app:contentInsetRight="0dp"
                    app:contentInsetStart="0dp"
                    app:layout_collapseMode="pin"
                    app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

                    <LinearLayout
                        android:id="@+id/header"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/appThemeColor_1"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/backArrowImgView"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_gravity="center_vertical|start"
                            android:layout_weight="0"
                            android:rotationY="@integer/angle_rtl_180"
                            android:src="@mipmap/ic_back_arrow"
                            android:tint="@color/white" />

                        <com.view.MTextView
                            android:id="@+id/titleTxtView"
                            android:layout_width="292dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1"
                            android:gravity="center|start"
                            android:text=""
                            android:singleLine="true"
                            android:ellipsize="marquee"
                            android:textAlignment="viewStart"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_14ssp"
                            app:customTypeFace="@string/systemRegular"
                            android:visibility="visible" />

                        <ImageView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_gravity="center_vertical"
                            android:padding="7dp"
                            android:tint="@color/white"
                            android:visibility="gone" />

                        <ImageView
                            android:padding="@dimen/_2sdp"
                            android:id="@+id/infoImage"
                            android:layout_width="@dimen/_22sdp"
                            android:layout_height="@dimen/_22sdp"
                            android:layout_gravity="center"
                            android:layout_marginEnd="@dimen/_5sdp"
                            android:tint="@color/white"
                            app:srcCompat="@drawable/ic_information" />

                        <com.like.LikeButton
                            android:id="@+id/likeButton"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|end"
                            android:backgroundTint="@color/white"
                            android:textAlignment="viewEnd"
                            android:visibility="visible"
                            app:anim_scale_factor="2"
                            app:icon_size="@dimen/_20sdp"
                            app:is_enabled="true"


                            app:like_drawable="@drawable/ic_heart_on_fav_store_new"
                            app:unlike_drawable="@drawable/ic_heart_off_fav_store_new" />

                        <com.view.CounterFab

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:padding="7dp"
                            android:textSize="6sp"
                            android:tint="@color/white"
                            android:visibility="gone"
                            app:backgroundTint="@android:color/transparent"
                            app:borderWidth="0dp"
                            app:elevation="0dp"
                            app:fabSize="mini"
                            app:rippleColor="@android:color/transparent"
                            app:srcCompat="@drawable/cart_new" />


                        <ImageView
                            android:id="@+id/rightFoodImgView"
                            android:layout_width="@dimen/_23sdp"
                            android:layout_height="@dimen/_23sdp"
                            android:layout_gravity="center_vertical"
                            android:layout_marginEnd="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_5sdp"

                            android:tint="@color/white"
                            app:srcCompat="@drawable/ic_search_black_24dp" />
                    </LinearLayout>

                </androidx.appcompat.widget.Toolbar>

            </com.google.android.material.appbar.CollapsingToolbarLayout>


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:id="@+id/menuArea"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginBottom="@dimen/_5sdp"
                    android:background="#f1f1f1"
                    android:orientation="horizontal"
                    android:visibility="visible">

                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/menuRecyclerview"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:clipToPadding="false"
                        android:orientation="horizontal"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager" />
                </LinearLayout>

                <RelativeLayout
                    android:id="@+id/hideAppBarLayout"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_60sdp"
                    android:background="#95383737"
                    android:clickable="false"
                    android:visibility="gone" />

            </RelativeLayout>

        </com.google.android.material.appbar.AppBarLayout>


        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/resDetails"
            android:layout_marginTop="-15dp"
            android:background="@color/white"
            android:clipToPadding="false"
            android:visibility="gone" />

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <!--        <android.support.v4.widget.NestedScrollView-->
            <!--            android:id="@+id/nestedscroll"-->
            <!--            android:layout_width="match_parent"-->
            <!--            android:layout_height="wrap_content"-->
            <!--            android:layout_below="@+id/resDetails"-->
            <!--          -->

            <!--            android:clipToPadding="false"-->
            <!--            android:visibility="visible">-->

            <LinearLayout
                android:id="@+id/addviewsLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/_15sdp"
                android:orientation="vertical">


                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/resMenuRecyclerview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_15sdp"
                    android:clipToPadding="false"
                    android:paddingBottom="70dp"
                    app:layout_behavior="@string/appbar_scrolling_view_behavior" />

                <View
                    android:id="@+id/blankView"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_25sdp"
                    android:visibility="gone" />

            </LinearLayout>
            <!--        </android.support.v4.widget.NestedScrollView>-->


        </FrameLayout>


        <ProgressBar
            android:id="@+id/mProgressBar"
            style="?android:attr/progressBarStyleLarge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:visibility="gone" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="bottom">


            <LinearLayout
                android:id="@+id/bottomCartView"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_40sdp"
                android:layout_alignParentBottom="true"
                android:background="@color/appThemeColor_1"
                android:orientation="horizontal"
                android:visibility="gone">

                <com.andremion.counterfab.CounterFab
                    android:id="@+id/cartFoodImgView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:padding="6dp"
                    android:textSize="6sp"
                    android:tint="@color/white"
                    android:visibility="visible"
                    app:backgroundTint="@android:color/transparent"
                    app:badgeBackgroundColor="@color/white"
                    app:badgeTextColor="@color/black"
                    app:borderWidth="0dp"
                    app:elevation="0dp"
                    app:fabSize="mini"
                    app:rippleColor="@android:color/transparent"
                    app:srcCompat="@drawable/cart_new" />

                <com.view.MTextView
                    android:id="@+id/viewCartTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:clickable="false"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:paddingStart="10dp"
                    android:paddingEnd="10dp"
                    android:text="view cart"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_12ssp"
                    app:customTypeFace="@string/systemMediumFont" />

                <com.view.MTextView
                    android:id="@+id/itemNpricecartTxt"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_weight="1"
                    android:clickable="false"
                    android:gravity="center_vertical|end"
                    android:maxLines="2"
                    android:paddingStart="10dp"
                    android:text="1 item | $ 11.00"
                    android:textAlignment="viewEnd"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_20ssp"
                    app:customTypeFace="@string/systemSemiBold" />


            </LinearLayout>

            <LinearLayout
                android:id="@+id/ratingArea"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"


                android:layout_gravity="bottom"
                android:visibility="gone">

                <include layout="@layout/item_list_rating" />
            </LinearLayout>


        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/fabMenuLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentBottom="true"
            android:background="#CC000000"
            android:visibility="gone">

            <View
                android:id="@+id/menubackView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#33d4d2d2"
                android:clickable="true" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:layout_centerInParent="true">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_gravity="bottom"
                    android:layout_margin="10dp"
                    android:adjustViewBounds="true"
                    android:background="@drawable/transparent_border"
                    android:elevation="10dp"
                    android:orientation="vertical">

                    <ScrollView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">

                        <LinearLayout
                            android:id="@+id/menuContainer"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_margin="5dp"
                            android:orientation="vertical" />
                    </ScrollView>

                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/dialogsLayoutArea"
            android:layout_width="match_parent"

            android:layout_height="match_parent"

            android:clickable="true"
            android:fitsSystemWindows="true"
            android:translationZ="@dimen/_90sdp"
            android:visibility="gone">

            <View
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#33d4d2d2"
                android:clickable="true" />

            <LinearLayout
                android:id="@+id/dialogsLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="gone">

                <include layout="@layout/open_dialogs" />

            </LinearLayout>


        </RelativeLayout>


    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</androidx.swiperefreshlayout.widget.SwipeRefreshLayout>